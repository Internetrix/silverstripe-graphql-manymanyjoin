{
  "name": "internetrix/silverstripe-graphql-manymanyjoin",
  "description": "Adds graphql support for the join relationship on many many lists",
  "type": "silverstripe-vendormodule",
  "homepage": "https://gitlab.internetrix.net/silverstripe-modules/graphql-manymanyjoin",
  "keywords": [
    "silverstripe",
    "module",
    "graphql",
    "manymany",
    "join",
    "scaffolders"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Guy Watson",
      "email": "guy.watson@internetrix.com.au"
    }
  ],
  "support": {
    "issues": "https://gitlab.internetrix.net/silverstripe-modules/graphql-manymanyjoin/issues"
  },
  "require": {
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "~4.0",
    "silverstripe/graphql": "~1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
    "installer-name": "graphql-manymanyjoin"
  },
  "autoload": {
    "psr-4": {
      "Internetrix\\GraphQLManyManyJoin\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
